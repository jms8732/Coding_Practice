package programmers1;

//°Å½º¸§ µ·
import java.util.*;

public class problem_15 {
	static int count = 0;
	public static void main(String[] args) {
		long result = solution(2000);
		System.out.println(result);
	}
	 public static long solution(int n) {
	      long answer = 0;
	      int [] jump = {1,2};
	      reCombination(0,0,n,jump);
	      return count % 1234567;
	  }
	
	 private static void reCombination(int depth, int value, int n, int [] jump) {
		 if(value >= n)
		 {
			 if(value == n)
				 count++;
			 return;
		 }
		 
		 if(depth == jump.length)
			 return;
		 
		 reCombination(depth,value+jump[depth],n,jump);
		 reCombination(depth+1,value,n,jump);
	 }

}
