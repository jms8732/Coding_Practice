package dp;

//문자열 판별
import java.util.*;
import java.io.*;

public class problem_16500 {
	public static void main(String[] args) throws IOException{
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		
		String s = br.readLine();
		int N = Integer.parseInt(br.readLine());
		
		List<String> array[] = new ArrayList[N];
		
		for(int i =0 ; i < array.length ; i++) array[i] = new ArrayList<>();
		
		for(int i =0 ; i < N ; i++) array[i].add(br.readLine());
		
		boolean check = false;
		outter : for(int i = 1 ; i < N ; i++) {
			String cur = array[i].get(0);
			for(int j =0 ; j < array[i-1].size() ; j++) {
				String tmp = array[i-1].get(j);
				
				String front = tmp.concat(cur);
				String rear = cur.concat(tmp);
				
				if(front.equals(s) || rear.equals(s)) {
					check = true;
					break outter;
				}
				
				array[i].add(front);
				array[i].add(rear);
			}
		}
		
		if(check)
			System.out.println(1);
		else
			System.out.println(0);
	}
}
