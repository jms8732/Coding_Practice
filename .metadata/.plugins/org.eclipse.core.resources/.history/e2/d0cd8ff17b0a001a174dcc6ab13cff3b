package programmers;
//여행 경로
//맨처음 집합을 이용
import java.util.*;

public class problem_46 {
	public static void main(String[] args) {
		String[][] tmp = { {"ICN", "SFO"}, {"ICN", "ATL"}, {"SFO", "ATL"}, {"ATL", "ICN"}, {"ATL","SFO"} };
		String[] result = solution(tmp);
		for (String s : result)
			System.out.print(s + " ");
	}

	public static String[] solution(String[][] tickets) {
		List<Node> totalList = new ArrayList<>();
		
		for(int i =0 ; i< tickets.length ; i++) {
			String depart = tickets[i][0];
			String dest = tickets[i][1];
			totalList.add(new Node(depart,dest,null));
		}
		List<Node> headList = new ArrayList<>();
		
		for(int i =0 ; i< totalList.size() ; i++) {
			String depart = totalList.get(i).departure;
			String dest = totalList.get(i).destination;
			
			if(depart.equals("ICN")) {
				List<Node> tmpList = new ArrayList<>(totalList);
				for(int j =0 ; j < tmpList.size() ; j++) {
					if(depart.equals(tmpList.get(i).departure) && dest.equals(tmpList.get(i).destination)) {
						tmpList.remove(j);
						break;
					}
				}
				headList.add(new Node(depart,dest,tmpList));
			}
		}
		
		String [] result = new String[tickets.length +1];
		int idx =0 ;
		
		headList.sort(new Comparator<Node>() {

			@Override
			public int compare(Node o1, Node o2) { //목적지를 대상으로 오름차순 정렬
				// TODO Auto-generated method stub
				if(o1.destination.compareTo(o2.destination) < 0)
					return -1;
				else
					return 1;
			}
			
		});
		
		Queue<Node> queue = new LinkedList<>();
		queue.add(headList.get(0));
		Node last = null;
		while(!queue.isEmpty()) {
			Node tmp = queue.poll();
			String depart = tmp.departure;
			String dest = tmp.destination;
			List<Node> restNode = tmp.restNode;
			
			if(restNode.isEmpty()) {
				last = tmp;
				break;
			}
			
			for(int i=0 ; i< restNode.size() ; i++) {
				//다음 행선지
				String nextDepart = restNode.get(i).departure;
				String nextDest = restNode.get(i).destination;
				
				if(dest.equals(nextDepart)) {
					//목적지와 출발지가 같은 경우
					List<Node> tmpList = new ArrayList<>(restNode);
					tmpList.remove(i); 
					result[idx++] = depart;
					queue.add(new Node(nextDepart,nextDest,tmpList));
				}
			}
		}
		result[idx++] = last.departure;
		result[idx] = last.destination;
		return new String[1];
	}

	public static class Node {
		String departure;
		String destination;
		List<Node> restNode;

		public Node(String depart, String dest, List<Node> rest) {
			this.departure = depart;
			this.destination = dest;
			this.restNode = rest;
		}
	}
}
