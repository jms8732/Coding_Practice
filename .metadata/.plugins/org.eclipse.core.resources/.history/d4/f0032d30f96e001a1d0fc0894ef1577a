import java.util.*;

public class problem_15 {
	static int answer = -1;

	public static void main(String[] args) {
		int n = 12;
		int[] weak = { 1,3,4,9,10};
		int[] dist = { 3,5,7 };

		int result = solution(n, weak, dist);
		System.out.println(result);
	}

	public static int solution(int n, int[] weak, int[] dist) {
		for(int i = 1 ; i <= dist.length; i++) {
			int [] array = new int[i];
			if(permutation(0,dist,weak,array)) {
				
			}
		}
		return answer;
	}
	
	private static boolean simulation(int depth,int next, int [] dist, int [] weak, int [] array){
		if(depth == array.length) {
			
			
			return;
		}
		
		
		for(int i = next ; i < dist.length ; i++) {
			array[depth] = dist[i];
			if(simulation(depth+1,i+1,dist,weak,array))
				return true;
		}
		
		
		return false;
	}
}