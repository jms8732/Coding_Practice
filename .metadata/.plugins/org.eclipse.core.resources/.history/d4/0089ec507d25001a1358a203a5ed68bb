package programmers1;

//°Å½º¸§µ·
import java.util.*;

public class problem_15 {
	static int count = 0;

	public static void main(String[] args) {
		int[] money = { 1, 2, 5 };
		int result = solution(5, money);
		System.out.println(result);
	}

	public static int solution(int n, int[] money) {

		dfs(n, 0, money);
		return count;
	}

	public static void dfs(int target, int value, int[] money) {
		if (target <= value) {
			if (target == value)
				count++;
			return;
		}

		for (int i = 0; i < money.length; i++) {
			dfs(target, value + money[i], money);
		}
	}
}
