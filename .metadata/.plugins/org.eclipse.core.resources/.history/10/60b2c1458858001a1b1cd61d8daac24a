package bruteForce;

//날짜 계산
import java.util.*;

public class problem_1476 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		int E = sc.nextInt();
		int S = sc.nextInt();
		int M = sc.nextInt();

		int start = 1;
		while (true) {
			if ((start % 15) == E && (start % 28) == S && (start % 19) == M)
				break;
			start++;
		}
		System.out.println(start);
	}
}
