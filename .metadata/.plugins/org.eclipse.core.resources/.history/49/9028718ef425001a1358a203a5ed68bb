package programmers1;

//¸Ö¸® ¶Ù±â
import java.util.*;

public class problem_15 {
	static long count =0;
	public static void main(String[] args) {
		long result = solution(4);
		System.out.println(result);
	}

	public static long solution(int n) {
		int[] jump = {1,2};
		
		dfs(n,0,jump);
		return count % 1234567;
	}
	
	
	private static void dfs(int n, int value,int[] jump) {
		if(n <= value) {
			if(value == n)
				count++;
			return;
		}
		
		for(int i = 0 ; i< jump.length ; i++) {
			dfs(n,value+jump[i],jump);
		}
	}
}
