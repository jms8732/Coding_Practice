import java.util.*;

public class problem_15 {
	static int count = 0;
	public static void main(String[] args) {
		long result = solution(4);
		System.out.println(result);
	}
	 public static long solution(int n) {
	      long answer = 0;
	      int [] jump = {1,2};
	      int [] dp = new int[2000];
	      dp[0] = 1;
	      dp[1] = 2;
	      
	      for(int i = 2 ; i < dp.length ; i++) {
	    	  dp[i] = dp[i-1] + dp[i-2];
	      }
	      
	      
	      return dp[n-1] % 1234567
	      
	  }
	
	 
}
