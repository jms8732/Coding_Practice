package bitmask;

//외판원 순회

import java.util.*;
import java.io.*;

public class problem_2098 {
	static int[][] map, dp;
	static int N;

	public static void main(String[] args) throws IOException {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		N = Integer.parseInt(br.readLine());

		map = new int[N][N];
		dp = new int[N][1<<N];
		
		StringTokenizer st = null;
		for (int i = 0; i < N; i++) {
			st = new StringTokenizer(br.readLine());
			for (int j = 0; j < N; j++)
				map[i][j] = Integer.parseInt(st.nextToken());
		}
		
		for(int i[] : dp) {
			Arrays.fill(i, -1);
		}
		
		int result = tsp(0, 1);
		
		System.out.println(result);
	}

	private static int tsp(int current, int visited) {
		int tmp = dp[current][visited];
		if(tmp != -1)
			return tmp;
		
		if((visited == (1<<N)-1)) {
			//4곳을 다 방문한 경우
			if(map[current][0] == 0)
				return Integer.MAX_VALUE;
			return map[current][0];
			
		}
		
		
		dp[current][visited] = Integer.MAX_VALUE;
		
		for(int i=0 ; i <N ;i++) {
			if((visited & (1<<i)) == 0 && map[current][i] != 0) {
				//아직 방문하지 않은 지역
				dp[current][visited]= Math.min(dp[current][visited], map[current][i] + tsp(i,(visited | (1<<i))));
				
			}
		}
		
		return tmp;
	}
}
