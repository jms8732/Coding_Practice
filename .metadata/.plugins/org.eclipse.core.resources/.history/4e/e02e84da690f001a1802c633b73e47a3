package programmers1;

//입국 심사
import java.util.*;

public class problem_4 {
	public static void main(String[] args) {
		int [] tmp = {7,10};
		long result = solution(6,tmp);
		System.out.println(result);
	}

	public static long solution(int n, int[] times) {
		Arrays.sort(times); //시간을 오름차순으로 정렬
		long low =0 , high= times[times.length-1] * n;
		long mid = 0 ; //중간 값
		int count = 0;
		while(low < high) {
			count =0 ;
			mid = (low + high)/2;
			for(int i =0 ; i< times.length ; i++) {
				count += mid / times[i];	
			}
			
			if(count == n)
				break;
			
			if(count > n)
				high = mid; //개수가 클 경우에는 중간값을 최댓값으로 지정
			else
				low = mid; //개수가 작을 경우에는 중간값을 최소값으로 지정
			
		}
		
		return mid;
	}
}
