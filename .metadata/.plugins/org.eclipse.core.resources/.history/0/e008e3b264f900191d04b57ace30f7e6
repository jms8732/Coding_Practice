package samsung;
import java.util.*;
public class test {
	public static void main(String[] args) {
		Vector<Integer> v = new Vector<>();
		Queue<Integer> q = new LinkedList<>();
		for(int i = 0 ; i< 10 ; i++) {
			v.add(i);
			q.add(i);
		}
		
		for(int i = 0 ; i< v.size() ; i++) {
			int tmp = v.get(i);
			if(tmp % 2 == 0)
				v.remove(i);
		}
		
		for(int i =0 ; i< q.size() ; i++) {
			int tmp = q.peek();
			if(tmp % 2 == 0)
				q.poll();
		}
	}
}
