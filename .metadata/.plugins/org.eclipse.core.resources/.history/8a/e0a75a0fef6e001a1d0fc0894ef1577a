package programmers2;


public class test {
	public static void main(String[] args) {
		int [] array = {1,2,3,4};
		
		for(int i =1 ; i <= array.length ; i++) {
			int [] tmp = new int[i];
			
			permutation(0,array,tmp);
		}
	}
	
	private static void permutation(int depth , int [] array , int [] tmp) {
		if(depth == tmp.length) {
			for(int i= 0 ; i < tmp.length ; i++) {
				System.out.print(tmp[i] + " " );
			}
			System.out.println();
		}
		
		for(int i = depth ; i < array.length ; i++) {
			tmp[depth] = array[i];
			swap(depth,i,array);
			permutation(depth+1,array,tmp);
			swap(i,depth,array);
		}
	}
	private static void swap(int depth ,int i , int[] array) {
		int tmp = array[depth];
		array[depth]  = array[i];
		array[i]= tmp;
	}
}
