package greedy;

//동전 뒤집기
import java.util.*;
import java.io.*;

public class problem_1285 {
	static char[][] map;
	static boolean[] col, row;
	static int N;
	static int answer = Integer.MAX_VALUE;

	public static void main(String[] args) throws IOException {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));

		N = Integer.parseInt(br.readLine());
		map = new char[N][N];
		row = new boolean[N];
		col = new boolean[N];

		int count = 0;
		for (int i = 0; i < N; i++) {
			String tmp = br.readLine();
			for (int j = 0; j < N; j++) {
				map[i][j] = tmp.charAt(j);
			}
		}

		simulation(0);
		System.out.println(answer);
	}

	private static void simulation(int cur) {
		if (cur == N * N) {
			int count = 0;
			for (int i = 0; i < N; i++) {
				for (int j = 0; j < N; j++) {
					if (map[i][j] == 'T')
						count++;
				}
			}
			answer = Math.min(count, answer);

			return;
		}

		char[][] tmp = map.clone();
		for (int i = cur; i < N * N; i++) {
			int x = i / N;
			int y = i % N;

			// 뒤집지 않았을 경우
			simulation(i + 1);

			// 행을 뒤집은 경우
			if (!row[x]) {
				for (int j = 0; j < N; j++) {
					if (map[x][j] == 'T')
						map[x][j] = 'H';
					else
						map[x][j] = 'T';
				}
				row[x] = true;
				simulation(i + 1);
				row[x] = false;
			}
			map = tmp;

			// 열을 뒤집은 경우
			if (!col[y]) {
				for (int j = 0; j < N; j++) {
					if (map[j][y] == 'T')
						map[j][y] = 'H';
					else
						map[j][y] = 'T';
				}
				col[y] = true;
				simulation(i + 1);
				col[y] = false;
			}
			map = tmp;
		}
	}
}
