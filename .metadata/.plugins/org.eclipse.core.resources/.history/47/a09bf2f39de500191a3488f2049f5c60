package samsung;

import java.util.Scanner;

public class problem_12100 {
	static int[][] map;
	static int[] ud = { -1, 0, 1, 0 };
	static int[] rl = { 0, 1, 0, -1 };
	static int N;
	static int[][] tmpMap;
	static int result;
	static int big;
	static boolean visited[][];
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		N = scanner.nextInt();
		map = new int[N][N];
		tmpMap = new int[N][N];
		visited = new boolean[N][N];
		for (int i = 0; i < N; i++) {
			for (int j = 0; j < N; j++) {
				int tmp = scanner.nextInt();
				map[i][j] = tmp;
			}
		} // map Ã¤¿ò
		copyMap(tmpMap, map);
		int [][] tmpMap = new int[N][N];
		copyMap(tmpMap,map);
		dfs(tmpMap,0);
		System.out.println(big);
	}

	static void dfs(int [][] tMap ,int count) {
		if(count >5) {
			count--;
			return;
		}
		for (int i = 0; i < rl.length; i++) {
			
			int nx = ud[i];
			int ny = rl[i];
			for (int j = 0; j < N; j++) {
				for (int k = 0; k < N ; k++) {
					if(map[j][k] != 0)
						move(j, k, nx, ny);
				}
			}
			move3(nx,ny);
			
			isBig();
			int [][] ttmap = new int[N][N];
			System.out.println();
			show();
			System.out.println();
			copyMap(ttmap,tMap);
			copyMap(tMap,map);
			visited = new boolean[N][N];
			dfs(tMap,count+1);
			copyMap(map,ttmap);
		}
	}
	static void move3(int ud,int rl) {
		if(ud == -1 && rl == 0) {
			for(int i =0 ; i< N ; i++) {
				for(int j = 0 ; j< N ;j++) {
					move2(i,j,ud,rl);
				}
			}
		}else if(ud == 1 && rl == 0) {
			for(int i= N-1 ; i >= 0 ; i--) {
				for(int j =0 ; j< N ; j++) {
					move2(i,j,ud,rl);
				}
			}
		}else if(ud == 0 && rl == 1) {
			for(int i = 0 ; i< N ; i++) {
				for(int j = N-1 ; j >= 0 ; j--) {
					move2(i,j,ud,rl);
				}
			}
		}else if(ud == 0 && rl == -1) {
			for(int i = 0 ; i < N ; i++) {
				for(int j =0 ; j < N ; j++) {
					move2(i,j,ud,rl);
				}
			}
		}
	}
	static void show() {
		for(int i =0 ; i < N ; i++) {
			for(int j = 0 ; j< N ; j++) {
				System.out.print(map[i][j] + " ");
			}
			System.out.println();
		}
	}
	static void move2(int x ,int y,int ud,int rl) {
		int nx =0 ;
		int ny= 0;
		int cx = x;
		int cy = y;
		int tmp = map[cx][cy];
		while (true) {
			nx = x + ud;
			ny = y + rl;
			if (nx < 0 || nx > map.length -1 || ny < 0 || ny > map[nx].length -1) {
				nx -= ud;
				ny -= rl;
				break;
			} else if (map[nx][ny] == 0) {
				x = nx;
				y = ny;
			} else {
				nx -= ud;
				ny -= rl;
				break;
			}
		}
		if (map[nx][ny] == 0) 
		{
			map[cx][cy] = 0;
			map[nx][ny] = tmp;
			
		}
	}
	static void isBig() {
		for(int i= 0 ; i<  N ; i++) {
			for(int j =0 ; j < N ; j++) {
				big = Math.max(big, map[i][j]);
			}
		}
	}

	static void move(int x, int y, int ud, int rl) {
		int cx = x;
		int cy = y;
		int tmp  =map[cx][cy];
		int nx = 0, ny = 0;
		while (true) {
			nx = x + ud;
			ny = y + rl;
			if (nx < 0 || nx > map.length -1 || ny < 0 || ny > map[nx].length -1) {
				nx -= ud;
				ny -= rl;
				break;
			} else if (map[x][y] == map[nx][ny] || map[nx][ny] == 0) {
				x = nx;
				y = ny;
			} else {
				nx -= ud;
				ny -=rl;
				break;	
			}
		}
		if (map[cx][cy] == map[nx][ny] && !visited[nx][ny])
		{
			map[cx][cy] = 0;
			map[nx][ny] += tmp;
			if(nx != cx || ny != cy )
				visited[nx][ny] = true;
			
		}
	}

	static void copyMap(int[][] des, int[][] src) {
		for (int i = 0; i < src.length; i++) {
			for (int j = 0; j < src[i].length; j++) {
				des[i][j] = src[i][j];
			}
		}
	}
}
