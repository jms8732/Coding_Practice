package programmers1;

//°Å½º¸§ µ·
import java.util.*;

public class problem_15 {
	static int count = 0;
	public static void main(String[] args) {
		long result = solution(5);
		System.out.println(result);
	}
	 public static long solution(int n) {
	      long answer = 0;
	      int [] jump = {1,2};
	      reCombination(0,n,jump);
	      return count % 1234567;
	  }
	
	 private static void reCombination (int value, int n, int [] jump) {
		 if(value >= n)
		 {
			 if(value == n)
				 count++;
			 return;
		 }
		 
		 
		 for(int i =0 ; i< jump.length; i++) {
			 reCombination(value+jump[i],n,jump);
		 }
	 }

}
