package programmers;


import java.util.*;
//소수 찾기
public class problem_14 {
	public static void main(String[] args) {
		int result = solution("17");
		System.out.println(result);
	}

	public static int solution(String numbers) {
		HashSet<Integer> set = new HashSet<>();
		int idx = 0;
		for(int i =0 ; i < numbers.length() ; i++) {
			String value = numbers.substring(i,i+1);
			dfs(numbers,set,value,idx+1);
		}
		

		return set.size();
	}
	
	public static void dfs(String numbers, HashSet<Integer> set, String value, int idx) {
		if(value.length() == numbers.length())
		{
			int tmp = Integer.parseInt(value);
			if(isPrime(tmp)) {
				set.add(tmp);
			}
			return ;
		}
		if(isPrime(Integer.parseInt(value))) //소수인지 판단
			set.add(Integer.parseInt(value));
		
		for(int i = idx ; i < numbers.length()  ; i++) {
			char c= numbers.charAt(i);
			StringBuilder sb = new StringBuilder(value);
			sb = sb.append(c);
			dfs(numbers,set,sb.toString(),idx+1);
			sb = sb.deleteCharAt(idx);
		}
	}
	public static boolean isPrime(int c) {
		int tmp = c;
		
		for(int i = tmp-1 ; i >= 2; i --)
		{
			if(c % i == 0)
				return false;
		}
		return true;
	}
}
