package programmers;
//여행 경로
//맨처음 집합을 이용
import java.util.*;

public class problem_46 {
	public static void main(String[] args) {
		String[][] tmp = { {"ICN", "SFO"}, {"ICN", "ATL"}, {"SFO", "ATL"}, {"ATL", "ICN"}, {"ATL","SFO"} };
		String[] result = solution(tmp);
		for (String s : result)
			System.out.print(s + " ");
	}

	public static String[] solution(String[][] tickets) {
		List<Node> totalList = new ArrayList<>();
		
		for(int i =0 ; i< tickets.length ; i++) {
			String depart = tickets[i][0];
			String dest = tickets[i][1];
			totalList.add(new Node(depart,dest,null));
		}
		List<Node> headList = new ArrayList<>();
		
		for(int i =0 ; i< totalList.size() ; i++) {
			String depart = totalList.get(i).departure;
			String dest = totalList.get(i).destination;
			
			if(depart.equals("ICN")) {
				List<Node> tmpList = new ArrayList<>(totalList);
				List<Node> tmp = new ArrayList<>();
				tmp.add(new Node(depart,dest,null));
				tmpList.removeAll(tmp);
				headList.add(new Node(depart,dest,tmpList));
			}
		}
		
		return new String[1];
	}

	public static class Node {
		String departure;
		String destination;
		List<Node> restNode;

		public Node(String depart, String dest, List<Node> rest) {
			this.departure = depart;
			this.destination = dest;
			this.restNode = rest;
		}
	}
}
