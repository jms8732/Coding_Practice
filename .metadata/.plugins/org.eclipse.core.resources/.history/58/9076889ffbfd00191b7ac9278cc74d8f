package programmers;

//가장 큰수
import java.util.*;

public class problem_25 {
	public static void main(String[] args) {
		int tmp[] = { 3, 3};
		String result = solution(tmp);
		System.out.println(result);
	}

	public static String solution(int[] numbers) {
		List<String> v = new LinkedList<>();
		StringBuilder sb = new StringBuilder();
		for (int i = 0; i < numbers.length; i++) {
			v.add(Integer.toString(numbers[i])); // 벡터 값으로 변환
		}

		while(!v.isEmpty()) {
			int i =0;
			String value1 = v.get(i);
			int bigIdx = 0;
			for (int j = i + 1; j < v.size(); j++) {
				String value2 = v.get(j);
				int i1 = 0;
				int i2 = 0;

				while (true) {
					if (i1 == value1.length() - 1 && i2 == value2.length()-1)
						break;
					int v1 = value1.charAt(i1) - '0';
					int v2 = value2.charAt(i2) - '0';
					if (v1 > v2) { // 값이 크면
						bigIdx = i;
						break;
					} else if (v1 < v2) {
						bigIdx = j;
						break;
					} 
					// 다음 숫자로 전환
					i1 = (i1 + 1) % value1.length();
					i2 = (i2 + 1) % value2.length();

				}
			}
			sb.append(v.get(bigIdx)); // 값 붙임
			v.remove(bigIdx); // 해당 값 삭제
		}

		return sb.toString();
	}

}
