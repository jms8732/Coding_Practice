package programmers2;

//[2020 카카오공채] 외벽 점검
import java.util.*;

public class problem_15 {
	static int answer = -1;

	public static void main(String[] args) {
		int n = 12;
		int[] weak = { 1, 5, 6, 10 };
		int[] dist = { 1, 2, 3, 4 };

		int result = solution(n, weak, dist);
		System.out.println(result);
	}

	public static int solution(int n, int[] weak, int[] dist) {
		int [] array= new int[dist.length];
		repair(0,n,weak,dist,array);
		return answer;
	}

	private static void repair(int depth, int n, int[] weak, int[] dist, int [] array) {
		if(depth == dist.length) {
			for(int i =0 ; i< array.length; i++)
				System.out.print(array[i] + " ");
			System.out.println();
		}
		
		for(int i =depth ; i < dist.length ; i ++) {
			array[depth] = dist[i];
			swap(depth,i,array);
			repair(depth+1,n,weak,dist,array);
			swap(depth,i,array);
		}
	}

	private static void swap(int depth, int i, int[] array) {
		int tmp = array[depth];
		array[depth] = array[i];
		array[i] = tmp;
	}
}

