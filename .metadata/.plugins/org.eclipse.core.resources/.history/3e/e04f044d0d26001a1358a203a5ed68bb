package combination;

import java.util.*;

//중복 조합 만들기
public class test2 {
	public static void main(String[] args) {
		int[] array = {1,2,3};
		int[] value = new int[2];
		reCombination(value,array,0,0);
		
	}
	
	private static void reCombination(int[] value,int [] array, int depth,int idx ) {
		if(depth == value.length)
		{
			print(value);
			return;
		}
		if(idx == array.length)
			return;
		value[depth] = array[idx];
		reCombination(value,array,depth+1,idx);
		reCombination(value,array,depth,idx+1);
	}
	
	private static void print(int[] list) {
		for(int i : list)
			System.out.print(i +" " );
		System.out.println();
	}
}
