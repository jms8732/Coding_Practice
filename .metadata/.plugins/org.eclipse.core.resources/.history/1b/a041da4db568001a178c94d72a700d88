package search_algorithm;

//한윤정이 이탈리에 가서 아이스크림을 사먹는데
import java.util.*;
import java.io.*;

public class problem_2422 {
	static Set<String> set;
	static int[] array;
	static StringBuilder sb;
	static int answer;

	public static void main(String[] args) throws IOException {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));

		StringTokenizer st = new StringTokenizer(br.readLine());

		int N = Integer.parseInt(st.nextToken());
		int K = Integer.parseInt(st.nextToken());
		array = new int[N];

		for (int i = 0; i < N; i++)
			array[i] = i + 1;
		set = new HashSet<>();

		for (int i = 0; i < K; i++) {
			char[] tmp = br.readLine().toCharArray();
			char[] target = {tmp[0],tmp[2]};
			set.add(new String(target));
		}

		sb = new StringBuilder();
		iceCream(0, 0);

		System.out.println(answer);
	}

	private static void iceCream(int depth, int next) {
		if (depth == 3) {
			String target = sb.toString().trim();
			String a = target.substring(0, 2);
			String b = target.substring(1, target.length());
			String c = target.substring(0,1) + target.substring(2);

			if(!set.contains(a) && !set.contains(b) && !set.contains(c))
					answer++;
			return;
		}

		for (int i = next; i < array.length; i++) {
			sb.append(array[i]);
			iceCream(depth + 1, i + 1);
			sb.delete(sb.length() - 1, sb.length());
		}
	}
}
